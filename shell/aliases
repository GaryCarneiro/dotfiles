alias ll='ls -lh'
alias rm='rm -i'
alias mv='mv -i'
alias ..='cd ..'
alias la='ls -alh'
alias df='df -h'
alias du='du -sh .'
alias vi='vim'
alias ..='cd ..'

alias av='ping -a in.yahoo.com'
alias s='ssh carneiro@s'
alias rssh='/Users/gcarneiro/git/SSHProfileCopy/bin/rssh.rb'
